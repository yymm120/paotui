{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Paotui Delivery",
  "version": "1.0.0",
  "identifier": "com.paotui.delivery",
  "build": {
    "beforeBuildCommand": "bun run build",
    "devUrl": "http://192.168.10.107:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Paotui Delivery",
        "width": 375,
        "height": 812,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "category": "Business",
    "copyright": "Â© 2024 Paotui Team",
    "createUpdaterArtifacts": false,
    "externalBin": [],
    "icon": ["icons/icon.ico"],
    "longDescription": "Professional delivery service mobile application for couriers and delivery drivers.",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": ""
    },
    "resources": [],
    "shortDescription": "Delivery service mobile app",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "android": {
      "minSdkVersion": 21,
      "versionCode": 1
    }
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "fs": {
      "requireLiteralLeadingDot": false
    }
  }
}
